{
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "impliedStrict": true,
      "experimentalObjectRestSpread": true,
      "jsx": true
    }
  },

    "env": {
        "browser": true,
         "es6": true,
         "amd": true,
         "node": true
    },

      //"extends": ["standard-jsx"],
      "extends": [ "eslint:recommended", "plugin:react/recommended" ],
      "plugins": [ "react" ],



      "rules": {
        "react/prop-types":0  //bad practice
      }

}
